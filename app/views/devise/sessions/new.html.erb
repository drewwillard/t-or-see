<div>
  <h2>Log in</h2>

  <div id="auth_form">
    <%= render partial: "users/sessions/form", locals: { 
      email: params[:email], 
      resource: resource || User.new(email: params[:email]), 
      resource_name: resource_name || :user 
    } %>
  </div>

  <div>
    <%= link_to "Back to events", root_path %>
  </div>
</div>
